{
    "name": "atayahmet/laravel-nestable",
    "type": "library",
    "description": "Laravel 5 nested categories library",
    "homepage": "https://github.com/atayahmet/nestable",
    "keywords": ["nested","nestable","nestedset","category","laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet ATAY",
            "email": "ahmet.atay@hotmail.com",
            "homepage": "http://www.atayahmet.com",
            "role": "Developer"
        }
    ],
    "repositories":
    [
            {
                "type": "vcs",
                "url": "https://github.com/rogerad/laravel-nestable"
            }
    ],
    "require": {
    "php": ">=5.5.5",
       "illuminate/support": "5.8.4.*|5.8.8.*|5.8.9.*|7.5.*|7.6.*|7.7.*|7.8.*|7.9.*|7.10.*|7.11.*|7.12.*|7.13.*|7.14.*|7.15.*|7.16.*|7.17.*|7.18.*|7.19.*|7.20.*|7.21.*|7.22.*|7.23.*|7.24.*|7.25|8.00.*|8.10.*|8.70.*|8.80.*|8.90.*|8.30.*|8.40.*|8.50.*",
        "illuminate/database": "5.8.4.*|5.8.8.*|5.8.9.*|7.5.*|7.7.*|7.8.*|7.9.*|7.10.*|7.11.*|7.12.*|7.13.*|7.14.*|7.15.*|7.16.*|7.17.*|7.18.*|7.19.*|7.20.*|7.21.*|7.22.*|7.23.*|7.24.*|7.25|8.00.*|8.10.*|8.70.*|8.80.*|8.90.*|8.20.*|8.30.*|8.40.*|8.50.*"
       
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Nestable\\": "src/",
            "Nestable\\Tests\\": "tests/"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nestable\\NestableServiceProvider"
            ],
            "aliases": {
                "Nestable": "Nestable\\Facades\\NestableService"
            }
        }
    },
    "minimum-stability": "dev"
}
