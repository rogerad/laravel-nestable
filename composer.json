{
    "name": "atayahmet/laravel-nestable",
    "type": "library",
    "description": "Laravel 5 nested categories library",
    "homepage": "https://github.com/atayahmet/nestable",
    "keywords": ["nested","nestable","nestedset","category","laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet ATAY",
            "email": "ahmet.atay@hotmail.com",
            "homepage": "http://www.atayahmet.com",
            "role": "Developer"
        }
    ],
    "repositories":
    [
            {
                "type": "vcs",
                "url": "https://github.com/rogerad/laravel-nestable"
            }
    ],
    "require": {
    "php": ">=8.0",
       "illuminate/support": "8.00.*|8.22.*|8.23.*|8.24.*|8.25.*|9.0.*|9.00.*|9.10.*|9.12.*|9.14.*|9.16.*|9.18.*|9.20.*|9.24.*|9.25.*|9.27.*|9.29.*|9.32.*|9.34.*|9.36.*|9.90.*|9.30.*|9.40.*|9.50.*",   
        "illuminate/database": "8.00.*|8.16.*|8.17.*|8.18.*|8.19.*|8.20.*|8.21.*|8.22.*|8.23.*|8.24.*|8.25.*|9.0.*|9.00.*|9.10.*|9.10.*|9.12.*|9.14.*|9.16.*|9.18.*|9.20.*|9.24.*|9.25.*|9.27.*|9.29.*|9.32.*|9.34.*|9.36.*|9.90.*|9.30.*"
       
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Nestable\\": "src/",
            "Nestable\\Tests\\": "tests/"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nestable\\NestableServiceProvider"
            ],
            "aliases": {
                "Nestable": "Nestable\\Facades\\NestableService"
            }
        }
    },
    "minimum-stability": "dev"
}
